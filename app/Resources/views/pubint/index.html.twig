{% extends '@admintmp/Default/index.html.twig' %}
{% block body %}
    <h1>Pubints list</h1>
<div class="cart-box-main">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="table-main table-responsive">
                    <table class="table table-dark">
                        <thead class="thead-dark">
                <th>Id</th>
                <th>Nom</th>
                <th>Datedebutpubint</th>
                <th>Dateexppubint</th>
                <th>Actions</th>

            </tr>
        </thead>
        <tbody>
        {% for pubint in pubints %}
            <tr>
                <td><a href="{{ path('pubint_show', { 'id': pubint.id }) }}">{{ pubint.id }}</a></td>
                <td>{{ pubint.nom }}</td>
                <td>{% if pubint.datedebutpubint %}{{ pubint.datedebutpubint|date('Y-m-d') }}{% endif %}</td>
                <td>{% if pubint.dateexppubint %}{{ pubint.dateexppubint|date('Y-m-d') }}{% endif %}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('pubint_show', { 'id': pubint.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('pubint_edit', { 'id': pubint.id }) }}">edit</a>
                        </li>
                        <li>
                            <div class="fb-share-button" data-href="https://developers.facebook.com/docs/plugins/" data-layout="button_count" data-size="small"><a target="_blank" href="https://www.facebook.com/sharer/sharer.php?u=https%3A%2F%2Fdevelopers.facebook.com%2Fdocs%2Fplugins%2F&amp;src=sdkpreparse" class="fb-xfbml-parse-ignore"><i class="fab fa-facebook-f"></i>

                                </a></div>
                        </li>
                        <li>
                            <div class="fb-share-button" data-href="https://developers.facebook.com/docs/plugins/" data-layout="button_count" data-size="small"><a href="https://twitter.com/intent/tweet?screen_name=TwitterDev&ref_src=twsrc%5Etfw" class="twitter-mention-button" data-show-count="false">Tweet to @TwitterDev</a><script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script></div>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
    </div>
            </div></div></div></div>

    <ul>
        <li>
            <a href="{{ path('pubint_new') }}">Create a new pubint</a>
        </li>
    </ul>
{% endblock %}
