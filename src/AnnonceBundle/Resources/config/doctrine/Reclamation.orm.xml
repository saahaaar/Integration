<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AnnoncesBundle\Entity\Reclamation" table="reclamation">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="objet" type="string" column="Objet" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="text" type="string" column="text" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="etat" type="string" column="etat" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="date" type="datetime" column="date" nullable="false"/>
  </entity>
</doctrine-mapping>
