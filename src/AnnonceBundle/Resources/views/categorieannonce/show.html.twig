{% extends 'index.html.twig' %}

{% block content %}
    <h1>Categorieannonce</h1>

    <table>
        <tbody>
            <tr>
                <th>Categorie</th>
                <td>{{ categorieannonce.categorie }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ categorieannonce.id }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('cat_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('cat_edit', { 'id': categorieannonce.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
